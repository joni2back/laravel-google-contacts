{
    "name": "reubenwedson/laravel-google-contacts",
    "description": "Easy access Google Contacts from Laravel Application",
    "keywords": [
        "reubenwedson",
        "google-contacts"        
    ],
    "homepage": "https://github.com/reubenwedson/laravel-google-contacts",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Reuben Wedson",
            "email": "reubenwedson@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Alpha Olomi",
            "email": "alphaolomi@gmail.com",
            "role": "Developer"
        }

    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.*|^6.*|^7.*|^8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Reubenwedson\\LaravelGoogleContacts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reubenwedson\\LaravelGoogleContacts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
